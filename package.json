{
  "name": "root",
  "version": "1.3.15",
  "private": true,
  "engines": {
    "node": "22"
  },
  "scripts": {
    "start": "bash -c 'source venv/bin/activate; backstage-cli repo start; deactivate'",
    "build:backend": "yarn workspace backend build",
    "build:all": "backstage-cli repo build --all",
    "build-image": "yarn workspace backend build-image",
    "tsc": "tsc",
    "tsc:full": "tsc --skipLibCheck false --incremental false",
    "clean": "backstage-cli repo clean",
    "test": "backstage-cli repo test",
    "test:all": "backstage-cli repo test --coverage",
    "test:e2e": "bash -c 'source venv/bin/activate; playwright test; deactivate'",
    "fix": "backstage-cli repo fix",
    "lint": "backstage-cli repo lint --since origin/main",
    "lint:all": "backstage-cli repo lint",
    "prettier:check": "prettier --check .",
    "new": "backstage-cli new"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "plugins/*"
    ]
  },
  "devDependencies": {
    "@backstage/cli": "^0.35.4",
    "@backstage/e2e-test-utils": "^0.1.2",
    "@jest/environment-jsdom-abstract": "^30.0.0",
    "@playwright/test": "^1.32.3",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jsdom": "^27.1.0",
    "node-gyp": "^10.0.0",
    "prettier": "^2.3.2",
    "typescript": "~5.4.0"
  },
  "resolutions": {
    "@backstage/app-defaults/@backstage/core-app-api": "~1.19.5",
    "@backstage/cli-common": "~0.1.17",
    "@backstage/plugin-techdocs-node": "~1.14.1",
    "@octokit/plugin-paginate-rest": "^13.0.0",
    "@octokit/plugin-rest-endpoint-methods": "^16.0.0",
    "@octokit/request": "^8.0.0",
    "@octokit/request-error": "^5.0.0",
    "@playwright/test": "~1.55.1",
    "@smithy/config-resolver": "~4.4.5",
    "diff": "~4.0.4",
    "fast-redact": "3.5.0",
    "fast-xml-parser": "~5.3.6",
    "form-data@^2.5.0": "2.5.5",
    "form-data@^2.3.2": "2.5.5",
    "form-data@^4.0.3": "4.0.4",
    "js-yaml": "~4.1.1",
    "jsonpath": "~1.2.1",
    "jwa": "~1.4.2",
    "jws": "~3.2.3",
    "koa": "~2.16.0",
    "lodash": "~4.17.23",
    "lodash-es": "~4.17.23",
    "markdown-it": "~14.1.1",
    "node-forge": "~1.3.2",
    "prismjs": "1.30.0",
    "qs": "~6.14.2",
    "react-router": "~6.30.2",
    "react-router-dom": "~6.30.2",
    "tar": "~7.5.8",
    "tar-fs": "2.1.4",
    "undici": "~7.18.2",
    "vm2": "~3.10.3"
  },
  "prettier": "@backstage/cli/config/prettier",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.12.0"
}
